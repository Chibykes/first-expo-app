{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    // "jsx": "react",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": ["./components/*"],
      "@assets/*": ["./assets/*"],
      "@hooks/*": ["./hooks/*"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
